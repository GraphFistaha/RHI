#version 450layout(location = 0) out vec2 fragUV;layout(binding = 1) uniform UniformBufferObject{    mat3 transform;} ubo;vec2 positions[6] = vec2[](    vec2(-1.0, -1.0),    vec2(1.0, -1.0),    vec2(1.0, 1.0),    vec2(1.0, 1.0),    vec2(-1.0, 1.0),    vec2(-1.0, -1.0));void main() {    gl_Position = vec4(vec3(positions[gl_VertexIndex], 0.0) * ubo.transform, 1.0);    fragUV = positions[gl_VertexIndex];}